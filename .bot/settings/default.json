{
	"filter": "filter.json",
	"filter_size": "384x384",
	"roll_separator": "\\s+(?:\\|\\||or|или)\\s+",
	"aliases": {
		"^/test\\b": "/echo test"
	},
	"auto_create_private_context": false,
	"search_enabled": true,
	"download": {
		"video": false,
		"video_note": true,
		"audio": false,
		"voice": true,
		"document": false,
		"photo": false,
		"sticker": false
	},
	"on_add_self": [
		"add_self"
	],
	"on_add_user": [
		"add_user"
	],
	"on_add_bot": [
		"add_bot"
	],
	"on_remove_user": [
		"remove_user"
	],
	"on_remove_bot": [
		"remove_bot"
	]
}
