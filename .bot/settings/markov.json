{
    "storage": {
        "state_separator": " "
    },
    "markov": {
        "parser": {
            "__class__": "Parser",
            "state_sizes": [ 1, 2, 3 ],
            "reset_on_sentence_end": true
        },
        "scanner": {
            "__class__": "RegExpScanner",
            "case": "preserve",
            "expr": "(?i)(?:(?P<end>\\|\\|\\|)|(?P<word>(?:>>[0-9]+(?:\\s*\\(OP\\))?|[^\\s]*@[^\\s]*|[a-z]+://[^\\s]*|[^\\w\\s]+|\\w+)))",
            "default_end": "|||"
        },
        "formatter": {
            "__class__": "Formatter",
            "case": "preserve",
            "end_chars": null,
            "default_end": null,
            "replace": [
                ["\\s+", " "],
                ["\\s*(?:///|\\|\\|\\|)\\s*", "\n"],
                ["([a-z]+://\\w+)\\.", "\\1\u1234", "io"],
                ["(@[^\\s]+)\\.", "\\1\u1234", "o"],
                ["(\\w+)\\.(\\w+@)", "\\1\u1234\\2", "o"],
                ["(\\w+)@(\\w+\\.\\w+)", "\\1\u1236\\2"],
                [">>([0-9]+)", "\u1235\\1"],
                ["\\s*([^@\\w\\s]+)\\s*", "\\1"],
                ["([,.?!])(\\w)", "\\1 \\2"],
                ["([\\w,.?!])([[({<]@+)", "\\1 \\2"],
                ["([])}>]+)(\\w)", "\\1 \\2"],
                ["(\\w)([-+*]+)(\\w)", "\\1 \\2 \\3"],
                ["\u1234", "."],
                ["\u1235", ">>"],
                ["\u1236", "@"],
                ["\\s+\\n", "\\n"],
                ["\\s*\\n*$", "\\n"]
            ]
        }
    }
}
